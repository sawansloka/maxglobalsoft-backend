{
  "env": {
      "browser": true,
      "es6": true,
      "node": true
    },
  "extends": ["airbnb-base", "prettier"],
  "plugins": ["prettier"],
  "rules": {
    "no-underscore-dangle" : "off",
    "func-names": "off",
    "no-use-before-define":"off",
    "no-console": "off",
    "consistent-return": "off",
    "prefer-rest-params": "off"
  }
}